pipeline{
    agent any
    stages {
        stage('gitcheckout'){
            steps {
                git branch: 'main', url: 'https://github.com/michaelgatta/henry.git'
            }
        }

        stage('dockerbuild'){
            steps {
                sh 'docker build -t michaelgat/timeserver.'
            }
        }

        stage('authenticate'){
            steps {
                sh 'echo auth'
            }
        }

        stage('push to regisry'){
            steps {
                sh 'echo push'
            }
        }

        stage('clean'){
            steps {
                sh 'echo cleanup'
            }
        }

        
    }
}